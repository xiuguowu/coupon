page{
  background-color: #f6f6f6;
}
.flexTop{
position:fixed;
left: 0;
width: 100%;
z-index: 2;
}
.top{
  background-color: #fff;
  display: flex;
 justify-content: space-between;
 align-items: center;
  padding: 20rpx 30rpx ;
}
.flexE{
  display: flex;
  align-items: center;
}
.head image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  display: inline-block;
  margin-right: 10rpx;
}
.nickname{
  font-size: 30rpx;
  color: #333;
  display: flex;
  align-items: center;
  /* line-height: 48rpx; */
}
.tag{
  font-size: 22rpx;
  color: #fff;
  padding: 2rpx 7rpx;
  display: inline-block;
  background-color: #4AD2CF;
  border-radius: 6rpx;
  margin-left: 10rpx;
}

.slogan image{
width: 379rpx;
height: 40rpx;
display: inline-block;
/* margin-left: 30rpx; */
}
.swiper {
  width: 100%;
  height: 260rpx;
  overflow: hidden;
}
.swiper image{
width: 100%;
height: 260rpx;
}
.effective{
  padding: 420rpx 30rpx 10rpx 30rpx;
}
.item{
position: relative;
margin-bottom: 30rpx;
}
.item_bg{
  height: 256rpx;
}
.item_bg image{
  width: 100%;
  height: 256rpx;
}
.limitCoupon{
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  /* position: relative; */
}
.limitCouponText{
  position: absolute;
  right: 8rpx;
  top: 2rpx;
  font-size: 20rpx;
  color: #fff;
}
.limitCoupon image{
width: 78rpx;
height: 32rpx;
}
.item_contentBg{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
}
.item_content{
  padding: 30rpx 30rpx 20rpx 30rpx;
}
.item_contentT{
  display: flex;
  justify-content: space-between;
  
}
.item_content_T{
  width: 480rpx;
  color: 32rpx;
  color: #333;
  overflow: hidden;
white-space: nowrap;
text-overflow: ellipsis;
margin-bottom: 20rpx;
}
.item_content_S{
  font-size: 24rpx;
  color: #888;
}
.item_content_B{
  font-size: 24rpx;
  color: #8C8C8C;
  /* border-top: 1rpx dotted #999; */
  padding-top:44rpx ;
  display: flex;
  justify-content: space-between;
  align-items: center;
 
}
.item_content_BT{
  display: inline-block;
  width: 475rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.line{
    position: absolute;
    top: 150rpx;
    left: 20rpx;
    width: 650rpx;
height: 1rpx;
background-image: linear-gradient(to right, #E5E5E5 0%, #E5E5E5 50%, transparent 50%);
background-size: 30rpx 1rpx;
background-repeat: repeat-x;
}
.details{
  display: inline-block;
  padding: 8rpx 24rpx;
  background-color: #31D4DB;
  border-radius: 30rpx;
  color: #fff;
}
.item_content_R{
  text-align: right;
}

.numS{
  font-size: 22rpx;
  color: #31D4DB;
  display: inline-block;
  margin: 0 10rpx;
}
.numB{
  font-size: 56rpx;
  color: #31D4DB;
}
.availableN{
  padding: 0 30rpx;
}
.availableN_T{
  font-size: 22rpx;
  color: #B6B6B6;
  margin-top: 10rpx;
  margin-bottom: 34rpx;
  position: relative;
  text-align: center;
}
.availableN_T::after{
  content: '';
  position: absolute;
  top: 50%;
  left: 50rpx;
  width: 150rpx;
  height: 1rpx;
  background-color: #E5E5E5;
}
.availableN_T::before{
  content: '';
  position: absolute;
  top: 50%;
  right: 50rpx;
  width: 150rpx;
  height: 1rpx;
  background-color: #E5E5E5;
}
.availableN .item_content_T{
  color: #E5E5E5;
}
.availableN .item_content_S{
  color: #E5E5E5;
}
.availableN .numB{
  color: #E5E5E5;
}
.availableN .numS{
  color: #E5E5E5;
}
.availableN .item_content_B{
  margin-top: 6rpx;
  color: #E5E5E5;
}
.invalid{
  position:absolute;
  top: 50rpx;
  right: 113rpx;
  z-index: 3;
}
.invalid image{
  width: 130rpx;
  height: 110rpx;
}
.customerService{
  position: fixed;
  top:368rpx;
  right: 0;
  width: 146rpx;
height: 80rpx;
background: #FFFFFF;
box-shadow: 0 6rpx 12rpx rgba(0, 0, 0, 0.16);
opacity: 1;
border-radius: 40rpx 0 0 40rpx;
display: flex;
align-items: center;
justify-content: center;
color: #434343;
font-size: 28rpx;
z-index: 9;
}
.customerService image{
  width: 44rpx;
  height: 44rpx;
  display: inline-block;
  margin-right: 8rpx;
}