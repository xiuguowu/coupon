/* pages/home/home/index.wxss */
Page{box-sizing:border-box;background:#FFFFFF;}
.container-body{display:block}
.particulars-hader{display:flex;flex-direction:row;flex-wrap:nowrap;justify-content:start;align-items:center;padding:44rpx 0rpx 44rpx 68rpx;border-bottom:2rpx solid #F1F1F1;}
.particulars-photo{width:120rpx;height:120rpx;overflow:hidden;border-radius:60rpx;-webkit-border-radius:60rpx;-moz-border-radius:60rpx;-ms-border-radius:60rpx;-o-border-radius:60rpx;}
.particulars-photo image{width:100%;height:100%;}
.header-logo{width:100%;height:100%;}
.particulars-JB-title{padding-left:60rpx;}
.particulars-JB-title text{font-size:30rpx;color:#333333;display:block;line-height:50rpx;}
/* */
.particulars-content-box{margin-left:30rpx;margin-right:30rpx;}
.particulars-sample-HP{margin-top:40rpx;padding-top:45rpx;padding-bottom:18rpx;text-align:center;border-radius:20rpx;background:rgba(255,255,255,1);box-shadow:0px 6px 16px 0px rgba(217,217,217,0.5);border:1px solid rgba(239,239,239,1);display:flex;flex-direction:column;justify-content:center;align-items:center;}
.particulars-image{display:block;width:100%;}
.particulars-image image{width:150rpx;height:150rpx;}
.particulars-text-J{width:100%;border-top:2rpx solid #F1F1F1;}
.particulars-text-J text{font-size:32rpx;color:#333333;}

.mask-modal_box{position:fixed;background:rgba(255,255,255,1);left:0rpx;right:0rpx;top:0rpx;bottom:0rpx;}
